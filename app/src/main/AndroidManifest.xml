<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.projects.pupus.dbintern" >
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_rounded"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" android:theme="@style/FancyImage" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Hauptmenue"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Sperrliste"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Casino"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Rabatt"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Ueber"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Changelog"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Anmerkung"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>